{
  "version": "4.0.0-*",
  "description": "Raven.Client is the client library for accessing RavenDB",
  "authors": [ "Hibernating Rhinos" ],
  "tags": [ "database", "nosql", "doc db" ],
  "projectUrl": "https://ravendb.net",

  "compilationOptions": {
    "allowUnsafe": true,
    "define": [ "PORTABLE" ]
  },
  "compile": [ "**/*.cs", "../CommonAssemblyInfo.cs", "../../Imports/Newtonsoft.Json/Src/Newtonsoft.Json/**/*.cs" ],
  "compileExclude": [ "../../Imports/Newtonsoft.Json/Src/Newtonsoft.Json/Properties/AssemblyInfo.cs" ],

  "dependencies": {
    "Microsoft.CSharp": "4.0.1-*",
    "Microsoft.Framework.ConfigurationModel": "1.0.0-*",
    "NETStandard.Library": "1.0.0-*",
    "Sparrow": "4.0.0-*",
    "System.Collections.Specialized": "4.0.0-*",
    "System.Diagnostics.Process": "4.1.0-*",
    "System.Dynamic.Runtime": "4.0.11-*",
    "System.Linq.Expressions": "4.0.11-*",
    "System.Linq.Queryable": "4.0.0-*",
    "System.Net.Http": "4.0.0-*",
    "System.Net.Http.WinHttpHandler": "4.0.0-*",
    "System.Net.NameResolution": "4.0.0-*",
    "System.Net.WebHeaderCollection": "4.0.1-*",
    "System.Net.WebSockets.Client": "4.0.0-*",
    "System.Runtime.Serialization.Primitives": "4.1.0-*",
    "System.Security.Cryptography.Algorithms": "4.0.0-*",
    "System.Security.Cryptography.Cng": "4.0.0-*",
    "System.Security.Cryptography.Primitives": "4.0.0-*",
    "System.Security.Principal": "4.0.0-*",
    "System.ServiceModel.Primitives": "4.1.0-*",
    "System.Threading.Thread": "4.0.0-*",
    "System.Xml.XDocument": "4.0.10-*"
  },

  "frameworks": {
    "dnxcore50": { }
  }
}
