{
  "version": "4.0.0-*",
  "description": "Raven.Client is the client library for accessing RavenDB",
  "authors": [ "Hibernating Rhinos" ],
  "tags": [ "database", "nosql", "doc db" ],
  "projectUrl": "https://ravendb.net",

  "dependencies": { "Sparrow": "4.0.0-*" },
  "compilationOptions": {"allowUnsafe": true},
  "compile": [ "**/*.cs", "../CommonAssemblyInfo.cs", "../../Imports/Newtonsoft.Json/Src/Newtonsoft.Json/**/*.cs" ],
  "compileExclude": [ "../../Imports/Newtonsoft.Json/Src/Newtonsoft.Json/Properties/AssemblyInfo.cs" ],


  "frameworks": {
    "dnxcore50": {
      "compilationOptions": { "define": [ "PORTABLE" ] },
      "dependencies": {
        "NETStandard.Library": "1.0.0-*",
        "System.Net.Http": "4.0.1-rc3-*",
        "System.IO.Compression": "4.1.0-rc3-*",
        "System.Dynamic.Runtime": "4.0.11-rc3-*",
        "System.Linq.Expressions": "4.0.11-rc3-*",
        "System.Linq.Queryable": "4.0.1-rc3-*",
        "System.Xml.XmlDocument": "4.0.1-rc3-*",
        "System.Xml.XDocument": "4.0.11-rc3-*",
        "System.Security.Cryptography.Primitives": "4.0.0-rc3-*",
        "System.Security.Cryptography.Algorithms": "4.0.0-rc3-*",
        "System.Security.Cryptography.Cng": "4.0.0-rc3-*",
        "System.Runtime.Serialization.Primitives": "4.1.0-rc3-*",
        "System.Collections.Specialized": "4.0.1-rc3-*",

        "System.Threading.Thread": "4.0.0-rc3-*",
        "System.IO.FileSystem.DriveInfo": "4.0.0-rc3-*",
        "Microsoft.Framework.ConfigurationModel": "1.0.0-*",
        "System.Diagnostics.Process": "4.1.0-rc3-*",
        "System.Net.NameResolution": "4.0.0-rc3-*"


      }
    }
  }
}
