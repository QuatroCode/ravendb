{
  "version": "4.0.0-*",
  "description": "Raven.Server is the database server for RavenDB",
  "authors": [ "Hibernating Rhinos" ],
  "tags": [ "database", "nosql", "doc db" ],
  "projectUrl": "https://ravendb.net",


  "compile": [ "**/*.cs", "../CommonAssemblyInfo.cs" ],

  "compilationOptions": {
    "emitEntryPoint": true,
    "allowUnsafe": true
  },

  "dependencies": {
    "Raven.Client": "4.0.0-*",
    "Sparrow": "4.0.0-*",
    "Voron": "4.0.0-*"
  },

  "commands": {
    "Raven.Server": "Raven.Server"
  },
  
  "frameworks": {
    "dnxcore50": {
      "dependencies": {

        "NETStandard.Library": "1.0.0-*",

        "Microsoft.AspNet.Hosting": "1.0.0-*",

        "Microsoft.AspNet.Hosting.Abstractions": "1.0.0-*",
        "Microsoft.AspNet.WebSockets.Server": "1.0.0-*"

      }
    }
  }
}
