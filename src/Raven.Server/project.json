{
  "version": "4.0.0-*",
  "description": "Raven.Server is the database server for RavenDB",
  "authors": [ "Hibernating Rhinos" ],
  "tags": [ "database", "nosql", "doc db" ],
  "projectUrl": "https://ravendb.net",


  "compile": [ "**/*.cs", "../CommonAssemblyInfo.cs" ],

  "compilationOptions": {
    "emitEntryPoint": true,
    "allowUnsafe": true,
    "warningsAsErrors": true
  },

  "dependencies": {
    "Lucene.Net": "3.0.4-*",
    "Lucene.Net.Contrib.Spatial.NTS": "3.0.4-*",
    "Raven.Client": "4.0.0-*",
    "Sparrow": "4.0.0-*",
    "Voron": "4.0.0-*"
  },

  "commands": {
    "Raven.Server": "Raven.Server"
  },

  "frameworks": {
    "dnxcore50": {
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-*",
        "System.Collections": "4.0.11-*",
        "System.Console": "4.0.0-*",
        "System.Linq": "4.1.0-*",
        "System.Threading": "4.0.11-*",
        "Microsoft.AspNetCore.Hosting": "1.0.0-*",
        "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-*",
        "Microsoft.Extensions.Configuration.FileProviderExtensions": "1.0.0-*",
        "Microsoft.Extensions.Configuration.Json": "1.0.0-*",
        "Microsoft.AspNetCore.WebSockets.Server": "0.1.0-*",

        "Microsoft.CodeAnalysis.CSharp": "1.1.0-*",
        "System.Runtime.Loader": "4.0.0-*"
      }
    }
  }
}
