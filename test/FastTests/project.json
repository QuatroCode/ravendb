{
  "version": "4.0.0-*",
  "commands": {
    "test": "xunit.runner.dnx"
  },
  "compilationOptions": {
    "allowUnsafe": true,
    "warningsAsErrors": true
  },
  "resource": [
    "**/*.json",
    "Voron/Bugs/Data/*.txt"
  ],

  "testRunner": "xunit",

  "dependencies": {
    "System.Linq.Expressions": "4.0.11-*",
    "System.Linq.Queryable": "4.0.0-*",
    "dotnet-test-xunit": "1.0.0-*",
    "NETStandard.Library": "1.0.0-*",
    "Raven.Client": "4.0.0-*",
    "Raven.Server": "4.0.0-*",
    "Sparrow": "4.0.0-*",
    "Voron": "4.0.0-*",
    "xunit": "2.2.0-*"
  },

  "frameworks": {
    "dnxcore50": { 
     "dependencies": { 
         "dotnet-test-xunit": "1.0.0-*" 
       }, 
       "imports": "portable-net451+win8" 
     }
  }
}
