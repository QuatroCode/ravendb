{
  "version": "4.0.0-*",
  "commands": {
    "test": "xunit.runner.aspnet"
  },
  "compilationOptions": {
    "allowUnsafe": true,
    "warningsAsErrors": true
  },
  "resource": [
    "**/*.json",
    "Voron/Bugs/Data/*.txt"
  ],

  "frameworks": {
    "dnxcore50": {
      "dependencies": {
        "xunit.runner.aspnet": "2.0.0-aspnet-rc2-16105",
        "xunit": "2.2.0-beta1-build3239",


        "NETStandard.Library": "1.0.0-*",
        "System.Threading.Tasks.Parallel": "4.0.1-rc3-*"



      }
    }
  },
  "dependencies": {
    "Raven.Client": "4.0.0-*",
    "Raven.Server": "4.0.0-*",
    "Sparrow": "4.0.0-*",
    "Voron": "4.0.0-*"
  }
}
